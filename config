#johatfie account
Host jon
    HostName github.com
    #User johatfie
    User git
    IdentityFile ~/.ssh/id_ed25519
    LogLevel DEBUG1
    #LogLevel DEBUG3
    IdentitiesOnly yes

#jhatf3_nike account
Host nike
    HostName github.com
